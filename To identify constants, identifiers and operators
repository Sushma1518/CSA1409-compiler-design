#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
	char a[100],i;
	printf("enter a string:");
	scanf("%s",a);
	int len=strlen(a);
	for(i=0;i<len;i++)
	{
		if(isalpha(a[i]))
		{
			printf("\n%c is alphabet.\n",a[i]);
		}
		else if(isdigit(a[i]))
		{
			printf("%c is digit.\n",a[i]);
		}
		else if(a[i]=='*' || a[i]=='-' || a[i]=='+' || a[i]=='=' || a[i]=='/' || a[i]=='%' || a[i]=='!' || a[i]=='(' || a[i]==')' )
		{
			printf("%c is operator\n",a[i]);
		}
	}
	return 0;
}

OUTPUT:
enter a string:a=a+b*100

a is alphabet.
= is operator

a is alphabet.
+ is operator

b is alphabet.
* is operator
1 is digit.
0 is digit.
0 is digit.
